<?xml version="1.0" encoding="utf-8" ?>
<odoo>
    <template id="report_binding">
        <t t-call="web.html_container">
            <t t-foreach="docs" t-as="o">
                <h2>binding</h2>
            </t>
        </t>
    </template>
    <template id="binding">
        <t t-call="web.html_container">
            <t t-foreach="docs" t-as="o">
                <t t-call="report_binding" />
            </t>
        </t>
    </template>

</odoo>